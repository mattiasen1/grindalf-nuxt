{
    "routes": [
      {
        "route": "/api/*",
        "allowedRoles": ["anonymous"]
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html"
    },
    "responseOverrides": {
      "404": {
        "rewrite": "/index.html"
      }
    },
    "globalHeaders": {
      "Access-Control-Allow-Origin": "*"
    },
    "forwarding": {
      "api": {
        "target": "https://secure.runescape.com",
        "pathRewrite": {
          "^/api": ""
        }
      }
    }
  }