{
    "routes": [
      {
        "route": "/api/*",
        "allowedRoles": ["anonymous"],
        "rewrite": "https://secure.runescape.com/:splat"
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html"
    },
    "responseOverrides": {
      "404": {
        "rewrite": "/index.html"
      }
    },
    "globalHeaders": {
      "Access-Control-Allow-Origin": "*"
    }
  }